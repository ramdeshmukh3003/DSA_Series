#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int n;
    cin>>n;
    for(int row=0;row<n;row++){
        for(int col=0;col<n;col++){
          char ch='A'+ col;
          cout<<ch<<" ";
        }
        cout<<endl;
    }

    return 0;
}

// star pattern #  26 -> sample output

//      A B C D E F 
//      A B C D E F 
//      A B C D E F 
//      A B C D E F 
//      A B C D E F 
//      A B C D E F